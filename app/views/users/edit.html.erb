<%= render 'layouts/errors', object: @user %>

<%= form_for @user, html: {method: :put} do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name, value:"" %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name, value: "" %>

  <%= f.label :phone %>
  <%= f.text_field :phone, value: "" %>

  <%= f.hidden_field :ip_address, value: @user.ip_address %>
  <% binding.pry %>

  <%= f.button :submit %>
<% end %>