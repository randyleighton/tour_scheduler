<%= render 'layouts/errors', object: @user %>

<%= form_for @user, html: {method: :put} do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :phone %>
  <%= f.text_field :phone %>

  <%= f.label :ip_address %>
  <%= f.text_field :ip_address %>

  <%= f.button :submit %>
<% end %>